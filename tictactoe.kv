<TicTacToeGrid>:
    id: grid_layout
    cols: 3
    rows: 3
    Button:
        font_size: 40
        on_release: app.button_pressed(self)
        coords: 0, 0
    Button:
        font_size: 40
        on_release: app.button_pressed(self)
        coords: 0, 1
    Button:
        font_size: 40
        on_release: app.button_pressed(self)
        coords: 0, 2
    Button:
        font_size: 40
        on_release: app.button_pressed(self)
        coords: 1, 0
    Button:
        font_size: 40
        on_release: app.button_pressed(self)
        coords: 1, 1
    Button:
        font_size: 40
        on_release: app.button_pressed(self)
        coords: 1, 2
    Button:
        font_size: 40
        on_release: app.button_pressed(self)
        coords: 2, 0
    Button:
        font_size: 40
        on_release: app.button_pressed(self)
        coords: 2, 1
    Button:
        font_size: 40
        on_release: app.button_pressed(self)
        coords: 2, 2
